{
  "name": "promise-api",
  "version": "0.0.0",
  "description": "Promise API Server",
  "author": {
    "name": "jinyongp",
    "email": "dev.jinyongp@gmail.com",
    "url": "https://jinyongp.dev"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "nest",
    "prisma": "prisma",
    "build": "nest build",
    "start:dev": "NODE_ENV=local nest start --exec \"bun run\" --watch",
    "migrate": "prisma migrate dev",
    "migrate:init": "prisma migrate dev --name init && prisma db seed",
    "migrate:stat": "prisma migrate status",
    "migrate:dev": "bunx env-cmd -e dev --use-shell ./scripts/migration.sh",
    "deploy": "sls deploy --verbose --force",
    "format": "prettier --write **/* --ext .ts",
    "lint": "DEBUG=eslint:cli-engine eslint \"src/**/*.ts\"",
    "lint:fix": "DEBUG=eslint:cli-engine eslint --fix \"src/**/*.ts\"",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage",
    "update": "bunx npm-check-updates -ui",
    "inthash": "bun run ./scripts/inthash.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.499.0",
    "@codegenie/serverless-express": "^4.13.0",
    "@nestjs-modules/ioredis": "^2.0.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/platform-ws": "^10.3.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/websockets": "^10.3.1",
    "@prisma/client": "5.10.2",
    "aws-lambda": "^1.0.7",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "inthash": "^3.0.3",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "mysql2": "^3.8.0",
    "remeda": "^1.46.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@types/aws-lambda": "^8.10.131",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prisma": "5.10.2",
    "serverless": "^3.38.0",
    "serverless-domain-manager": "^7.3.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "bun run ./prisma/seeds"
  }
}
