{
  "name": "promise-api",
  "version": "0.0.0",
  "description": "Promise API Server",
  "author": {
    "name": "jinyongp",
    "email": "dev.jinyongp@gmail.com",
    "url": "https://jinyongp.dev"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "nest",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "migration": "npm run migration:local",
    "migration:local": "dotenv -oe .env.local -- tsx ./scripts/migration.ts",
    "migration:test": "dotenv -oe .env.test -- tsx ./scripts/migration.ts",
    "migration:dev": "dotenv -oe .env.dev -- tsx ./scripts/migration.ts",
    "ssh-tunnel": "env-cmd -e dev --use-shell tsx ./scripts/ssh-tunnel.ts",
    "format": "prettier --write **/* --ext .ts",
    "lint": "DEBUG=eslint:cli-engine eslint \"src/**/*.ts\"",
    "lint:fix": "DEBUG=eslint:cli-engine eslint --fix \"src/**/*.ts\"",
    "lint:tsc": "tsc --noEmit",
    "test": "dotenv -oe .env.test -- jest",
    "coverage": "dotenv -oe .env.test -- jest --coverage",
    "update": "npx npm-check-updates -ui",
    "inthash": "dotenv -oe .env.local -- tsx ./scripts/inthash.ts",
    "init": "./scripts/initialize.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@codegenie/serverless-express": "^4.14.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-ws": "^10.3.3",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/websockets": "^10.3.3",
    "@prisma/client": "^5.11.0",
    "aws-lambda": "^1.0.7",
    "cache-manager": "^5.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.4.0",
    "express": "^4.18.3",
    "inthash": "^3.0.3",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
    "mysql2": "^3.9.2",
    "remeda": "^1.57.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/aws-lambda": "^8.10.136",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "chalk": "^5.3.0",
    "dotenv-cli": "^7.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "serverless": "^3.38.0",
    "serverless-domain-manager": "^7.3.8",
    "serverless-prune-plugin": "^2.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.7.1",
    "typescript": "5.4"
  },
  "prisma": {
    "seed": "tsx ./prisma/seeds"
  }
}
