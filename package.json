{
  "name": "promise-api",
  "version": "0.0.0",
  "description": "Promise API Server",
  "author": {
    "name": "jinyongp",
    "email": "dev.jinyongp@gmail.com",
    "url": "https://jinyongp.dev"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "nest",
    "build": "nest build",
    "dev": "nest start --watch",
    "sls:dev": "dotenv -oe .env.dev -- sls",
    "prisma": "dotenv -oe .env.local -- prisma",
    "migration": "npm run migration:local",
    "migration:local": "dotenv -oe .env.local -- tsx ./scripts/migration",
    "migration:test": "dotenv -oe .env.test -- tsx ./scripts/migration",
    "migration:dev": "dotenv -oe .env.dev -- tsx ./scripts/migration",
    "ssh-tunnel": "env-cmd -e dev --use-shell tsx ./scripts/ssh-tunnel.ts",
    "format": "prettier --write **/* --ext .ts",
    "lint": "DEBUG=eslint:cli-engine eslint \"src/**/*.ts\"",
    "lint:fix": "DEBUG=eslint:cli-engine eslint --fix \"src/**/*.ts\"",
    "test": "dotenv -oe .env.test -- jest",
    "coverage": "dotenv -oe .env.test -- jest --coverage",
    "update": "npx -y npm-check-updates -ui",
    "inthash": "dotenv -oe .env.local -- tsx ./scripts/inthash.ts",
    "generate-key-pair": "tsx ./scripts/generate-key-pair.ts",
    "init": "./scripts/initialize.sh"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.609.0",
    "@aws-sdk/client-s3": "^3.609.0",
    "@codegenie/serverless-express": "^4.14.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/platform-socket.io": "^10.3.10",
    "@nestjs/platform-ws": "^10.3.10",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/websockets": "^10.3.10",
    "@prisma/client": "^5.16.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-highlight": "^2.1.11",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "hbs": "^4.2.0",
    "inthash": "^3.0.4",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.10.2",
    "remeda": "^2.3.0",
    "rxjs": "^7.8.1",
    "sqids": "^0.3.0",
    "sql-highlight": "^6.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/aws-lambda": "^8.10.140",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "chalk": "^5.3.0",
    "dotenv-cli": "^7.4.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-jsdoc": "^48.5.2",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "path-to-regexp": "^7.0.0",
    "prettier": "^3.3.2",
    "prisma": "^5.16.1",
    "serverless": "^3.39.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-domain-manager": "^7.3.8",
    "serverless-prune-plugin": "^2.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.16.2",
